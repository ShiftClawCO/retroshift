{
  "default": {
    "paths": ["e2e/features/**/*.feature"],
    "require": ["e2e/steps/**/*.ts", "e2e/support/**/*.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "progress-bar",
      "html:e2e/reports/cucumber-report.html"
    ],
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "publishQuiet": true
  }
}
